    "version": "0.2.0",
    "configurations": [
        {
            "command": "npx tsc api/src/concat.ts --sourceMap",
            "name": "Compile TS",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/api/__tests__/concat.test.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "command": "npm start",
            "name": "Run",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "npm run react",
            "name": "Run React",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "NODE_ENV=development node scrapermongo.js",
            "name": "Debug winserter",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "NODE_ENV=development node scraper/scimmer.js",
            "name": "Debug Scimmer",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "NODE_ENV=development node scraper/index.js",
            "name": "Debug full scraper",
            "request": "launch",
            "type": "node-terminal"
        }
    ]
}